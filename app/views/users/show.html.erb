<h1>Created Events</h1>

<h2>Upcoming Events</h2>
<ul>
  <% @future_created_events.each do |event| %>
    <li>
      <%= render 'events/semi_display', event: event %>
    </li>
  <% end %>
</ul>

<h2>Past Events</h2>
<ul>
  <% @past_created_events.each do |event| %>
    <li>
      <%= render 'events/semi_display', event: event %>
    </li>
  <% end %>
</ul>

<%= link_to "Create New Event", new_event_path %>

